#ifndef myStackStuff_Stack_h
#define myStackStuff_Stack_h

const int stackSize = 10;

template<class T>
class Stack
{
private:
    T items[stackSize];
    int stackIndex;
public:
    Stack();
    void push(T);
    void pop();
    T peek();
    int size();
    bool isFull();
    bool isEmpty();
};

#endif
